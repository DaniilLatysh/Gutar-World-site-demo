doctype html
html(ng-app)
	head
		meta(charset='utf-8')
		title Alpha
		for style in styles
			link(rel='stylesheet', href='#{style}')
		for script in scripts
			script(src='#{script}')
	body
		#wrapper
			.page
				.header
					h1.name Gibson
					.logo
						a(href='#')
							img(height='100px', src='/images/GWL.png')
					nav
						ul.top-menu
								li
									a.button.grow(href='#') HOME
								li 
									a.button.grow(href='index') ABOUT US
								li
									a.button.grow(href='chat') CHAT
								li
									a.button.grow(href='video') VIDEO
								li
									a.button.grow(href='contact') CONTACT
								li
									a.button.grow(href='projects') PROJECTS
									if userid
											li
												a.button.grow(href='cobinet') COBINET
											li
												a.button.grow(href='logout') EXIT
										else
											li
												a.button.grow(href='reg') REGISTRATION | LOGIN
			div
				nav
					ul.aside-menu
							li
								a.button.grow(href='guitars') Guitars
							li
								a.button.grow(href='#') Guitar amps
							li
								a.button.grow(href='#') Guitar accessories
							li
								a.button.grow(href='#') Strings
							li
								a.button.grow(href='#') Drums and percussion
							li
								a.button.grow(href='#') Audio Equipment
			div.menu-right 
				.social.grow
					p 
						| ▼ Social here ▼
				.bord.grow
					.image-effect-fall-back
						.share-layer
							a.share-button.share-via-facebook(href='https://ru-ru.facebook.com/')
								i.fa.fa-facebook
									|Facebook
							a.share-button.share-via-twitter(href='https://twitter.com/')
								i.fa.fa-twitter
									|Twitter
							a.share-button.share-via-google(href='https://www.google.com/')
								i.fa.fa-google
									|Google+
							a.share-button.share-via-VK(href='https://www.vk.com/')
								i.fa.fa-VK
									|VKontakte
				.social.grow
					p 
						| Google Map
				div#map-canvas(href='location')
					
				
				
			div.content
				blockquote
						p
							| “Music should strike fire from the hearts of men”
							cite Ludwig van Beethoven
				
				block content
			.footer
				.copyright
					| © Copyright
				.contact-footer
					p
						| You are can contact us by phone: (706) 724-1172
				
